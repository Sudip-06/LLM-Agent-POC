PORT=7860

# Defaults if the UI doesn't send a model
DEFAULT_GROQ_MODEL=openai/gpt-oss-120b
DEFAULT_GEMINI_MODEL=gemini-2.5-flash

# Provider keys (for chat endpoints)
GROQ_API_KEY=your_groq_key_here
GEMINI_API_KEY=your_gemini_key_here

# Google Custom Search (for /api/search)
GOOGLE_API_KEY=your_google_api_key
GOOGLE_CSE_ID=your_cse_cx

# AI Pipe proxy
AIPIPE_URL=https://custom-aipipe-url.onrender.com/run
AIPIPE_AUTH=Bearer your_token_here

